{
  "name": "sunny.gg",
  "version": "1.0.0",
  "description": "My new website.",
  "main": "index.js",
  "scripts": {
    "predev": "npm run build-posts && npm run generate-version",
    "prebuild": "npm run build-posts && npm run generate-version",
    "prepare": "husky install",
    "dev": "next dev --port 8080",
    "build": "next build && next export -o dist && next-image-export-optimizer",
    "build-dry": "next build && next export -o .next/dry",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "next lint",
    "tsc": "tsc",
    "serve": "http-server dist --port 9090",
    "build-posts": "ts-node ./scripts/buildPostMap.ts",
    "create-post": "ts-node ./scripts/createPost.ts",
    "generate-version": "./scripts/generate-version.sh",
    "lint-posts": "prettier --check public/posts/**/*.md public/snippets/**/*.md",
    "lint-posts:fix": "prettier --write public/posts/**/*.md public/snippets/**/*.md"
  },
  "author": "Sunny Golovine",
  "license": "0BSD",
  "dependencies": {
    "@types/mapbox__rehype-prism": "^0.8.0",
    "clsx": "^1.2.1",
    "daisyui": "^2.24.0",
    "dayjs": "^1.11.5",
    "glob": "^8.0.3",
    "gray-matter": "^4.0.3",
    "lodash.uniq": "^4.5.0",
    "next": "^12.2.5",
    "next-image-export-optimizer": "^0.12.2",
    "next-mdx-remote": "^4.1.0",
    "next-seo": "^5.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@mapbox/rehype-prism": "^0.8.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/glob": "^8.0.0",
    "@types/lodash.uniq": "^4.5.7",
    "@types/node": "^18.7.14",
    "@types/prompts": "^2.0.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/react-headroom": "^2.2.2",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
}
